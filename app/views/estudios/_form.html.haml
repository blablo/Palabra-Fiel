-form_for @estudio do |f|
  =f.label :album
  =f.text_field :album
  %br
  =f.label :nombre
  =f.text_field :nombre
  %br
  =f.label :sintesis
  =f.text_area :sintesis
  %br
  =f.label :fecha
  =f.date_select :fecha
  %br
  =f.label :audio_url
  =f.text_field :audio_url, :size => "100px"
  %br
  =f.label :file_size
  =f.text_field :file_size
  %br

  =f.submit "Guardar"
